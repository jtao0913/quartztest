					<xs:element name="CarrierNumber" type="string"/>
					<xs:element name="CarrierVolumeNumber" type="string"/>
					<xs:element name="AbstractNumber" type="string"/>
					<xs:element name="ReferenceNumber" type="string"/>
					<xs:element name="Issue" type="string"/>
					<xs:element ref="Imprint"/>
					<xs:element name="NPLCode" type="string"/>
					<xs:element name="Class">
						<xs:complexType>
							<xs:simpleContent>
								<xs:extension base="string">
									<xs:attribute name="type"/>
								</xs:extension>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="Keyword" type="Text" maxOccurs="unbounded"/>
					<xs:element ref="ElectronicAccess"/>
					<xs:element name="DescriptiveText" type="Text"/>
					<xs:element name="CopyRight" type="Text"/>
					<xs:element ref="RichContent"/>
					<xs:element name="Text" type="Text"/>
				</xs:choice>
			</xs:choice>
			<xs:attribute name="id" type="string"/>
			<xs:attribute name="idRef" type="string"/>
			<xs:attributeGroup ref="language"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Serial">
		<xs:annotation>
			<xs:documentation>引用连续出版物，引证模块</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SerialTitle" type="Text" minOccurs="0">
					<xs:annotation>
						<xs:documentation>出版物标题</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SubTitle" type="Text" minOccurs="0">
					<xs:annotation>
						<xs:documentation>副标题</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="SubName" minOccurs="0" maxOccurs="unbounded"/>
