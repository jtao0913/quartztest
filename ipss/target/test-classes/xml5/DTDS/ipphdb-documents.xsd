						<xs:documentation>图像中文字的代码化内容</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Paragraph"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="lang"/>
			<xs:attribute name="status" type="string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="VerdictsDocument">
		<xs:annotation>
			<xs:documentation>司法判决信息</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Court">
					<xs:annotation>
						<xs:documentation>法庭</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CourtHouse">
								<xs:annotation>
									<xs:documentation>法院的具体名称</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="Text">
											<xs:attribute name="level" use="required">
												<xs:annotation>
													<xs:documentation>初级-district；中级-appeal；高级-high；最高-supreme</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:token">
														<xs:enumeration value="appeal"/>
														<xs:enumeration value="district"/>
														<xs:enumeration value="high"/>
														<xs:enumeration value="supreme"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="area" type="string" use="required">
												<xs:annotation>
													<xs:documentation>法院所在地区</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="number" type="string">
												<xs:annotation>
													<xs:documentation>法院的编号</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="SubBranch">
								<xs:annotation>
									<xs:documentation>审判厅</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="id"/>
									<xs:attribute ref="sequence"/>
									<xs:attributeGroup ref="language"/>
									<xs:attribute name="section" use="required">
										<xs:annotation>
											<xs:documentation>司法判决的法院的审判厅种类</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DecisionNumber" type="string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>复审或无效宣告的决定号</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="PatentInformation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>相关专利信息,与AppealDecision共享</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="LegalFileNumberDetails">
					<xs:annotation>
						<xs:documentation>司法判决的所有判决号信息</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="LegalFileNumber" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="Text">
											<xs:attribute ref="year"/>
											<xs:attribute name="number"/>
											<xs:attribute name="section"/>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="LegalCaseParties">
					<xs:annotation>
						<xs:documentation>判决相关人</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="LegalPlaintiffDetails">
								<xs:annotation>
									<xs:documentation>原告/上诉人信息</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="LegalPlaintiff" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="LegalLitigant">
														<xs:annotation>
															<xs:documentation>当事人信息</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element ref="LegalAttorneyDetails" minOccurs="0">
														<xs:annotation>
															<xs:documentation>法律代理人信息</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="id" type="string"/>
												<xs:attribute ref="sequence"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="LegalDefendantDetails">
								<xs:annotation>
									<xs:documentation>被告/被上诉人信息</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="LegalDefendant" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="LegalLitigant">
														<xs:annotation>
															<xs:documentation>当事人信息</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element ref="LegalAttorneyDetails" minOccurs="0">
														<xs:annotation>
															<xs:documentation>法律代理人信息</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="id" type="string"/>
												<xs:attribute ref="sequence"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="LegalThirdPartyDetails" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>相关第三人</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="LegalThirdParty" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="LegalLitigant" maxOccurs="unbounded"/>
													<xs:element ref="LegalAttorneyDetails" minOccurs="0"/>
												</xs:sequence>
												<xs:attribute name="id" type="string"/>
												<xs:attribute ref="sequence"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="JudgeDetails">
					<xs:annotation>
						<xs:documentation>审判员信息</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Judge" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="Person">
											<xs:attribute name="chief" default="no">
												<xs:simpleType>
													<xs:restriction base="xs:NMTOKEN">
														<xs:enumeration value="yes"/>
														<xs:enumeration value="no"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="deputy" default="no">
												<xs:simpleType>
													<xs:restriction base="xs:NMTOKEN">
														<xs:enumeration value="yes"/>
														<xs:enumeration value="no"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="assistant" default="no">
												<xs:simpleType>
													<xs:restriction base="xs:NMTOKEN">
														<xs:enumeration value="yes"/>
														<xs:enumeration value="no"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="assessor" default="no">
												<xs:simpleType>
													<xs:restriction base="xs:NMTOKEN">
														<xs:enumeration value="yes"/>
														<xs:enumeration value="no"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CourtReporterDetails">
					<xs:annotation>
						<xs:documentation>书记员</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CourtReporter" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="Person"/>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="VerdictDetail">
					<xs:annotation>
						<xs:documentation>判决的正文</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:choice>
							<xs:element ref="DocPage" maxOccurs="unbounded"/>
							<xs:sequence>
								<xs:element name="VerdictCaseIssue" minOccurs="0">
									<xs:annotation>
										<xs:documentation>司法判决正文的案由部分</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:choice maxOccurs="unbounded">
											<xs:element name="ComplainantOpinion">
												<xs:annotation>
													<xs:documentation>原告意见</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:choice maxOccurs="unbounded">
														<xs:element ref="Heading"/>
														<xs:element ref="Paragraph"/>
													</xs:choice>
												</xs:complexType>
											</xs:element>
											<xs:element name="PreviousAppealDecisionOpinion">
												<xs:annotation>
													<xs:documentation>复审的意见</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:choice maxOccurs="unbounded">
														<xs:element ref="Heading"/>
														<xs:element ref="Paragraph"/>
													</xs:choice>
												</xs:complexType>
											</xs:element>
											<xs:element name="PreviousVerdict">
												<xs:annotation>
													<xs:documentation>一审判决（二审中引用）</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:choice maxOccurs="unbounded">
														<xs:element ref="Heading"/>
														<xs:element ref="Paragraph"/>
													</xs:choice>
												</xs:complexType>
											</xs:element>
											<xs:element name="AppellantOpinion">
												<xs:annotation>
													<xs:documentation>上诉人的意见</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:choice maxOccurs="unbounded">
														<xs:element ref="Heading"/>
														<xs:element ref="Paragraph"/>
													</xs:choice>
												</xs:complexType>
											</xs:element>
											<xs:element name="AppelleeOpinion">
												<xs:annotation>
													<xs:documentation>被上诉人的意见</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:choice maxOccurs="unbounded">
														<xs:element ref="Heading"/>
														<xs:element ref="Paragraph"/>
													</xs:choice>
												</xs:complexType>
											</xs:element>
											<xs:element name="ThirdPartyOpinion">
												<xs:annotation>
													<xs:documentation>第三人意见</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:choice maxOccurs="unbounded">
														<xs:element ref="Heading"/>
														<xs:element ref="Paragraph"/>
													</xs:choice>
												</xs:complexType>
											</xs:element>
										</xs:choice>
									</xs:complexType>
								</xs:element>
								<xs:element name="FactCognizance" minOccurs="0">
									<xs:annotation>
										<xs:documentation>事实的认定</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:choice maxOccurs="unbounded">
											<xs:element ref="Heading"/>
											<xs:element ref="Paragraph"/>
										</xs:choice>
									</xs:complexType>
								</xs:element>
								<xs:element name="VerdictOpinion">
									<xs:annotation>
										<xs:documentation>判决的意见</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="VerdictReasoning" minOccurs="0">
												<xs:annotation>
													<xs:documentation>判决的理由</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:choice maxOccurs="unbounded">
														<xs:element ref="Heading"/>
														<xs:element ref="Paragraph"/>
													</xs:choice>
												</xs:complexType>
											</xs:element>
											<xs:element name="VerdictHolding" minOccurs="0">
												<xs:annotation>
													<xs:documentation>判决的内容</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:choice maxOccurs="unbounded">
														<xs:element ref="Heading"/>
														<xs:element ref="Paragraph"/>
													</xs:choice>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element name="VerdictDate" type="Date">
					<xs:annotation>
						<xs:documentation>判决日期</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="PublicationInformation" minOccurs="0"/>
				<xs:element ref="Abstract" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Keywords" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AccessoryDetails" minOccurs="0">
					<xs:annotation>
						<xs:documentation>复审决定基本信息的模块：审查决定的附件,与Paragraph共享</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="DecisionDrawings" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="sequence" use="required"/>
			<xs:attribute ref="lang"/>
			<xs:attribute name="fileReferenceId" type="string">
				<xs:annotation>
					<xs:documentation>文献中涉及的文件的编码</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="correctionCode" type="string">
				<xs:annotation>
					<xs:documentation>修正码</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="datePublication" type="Date">
				<xs:annotation>
					<xs:documentation>文献公布/公告日期</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute ref="status"/>
			<xs:attribute ref="verdictType" use="required"/>
			<xs:attribute name="appealed">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="yes"/>
						<xs:enumeration value="no"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="holdingType"/>
			<xs:attribute name="reasonType">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="substance"/>
						<xs:enumeration value="procedure"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="LegalLitigant">
		<xs:annotation>
			<xs:documentation>当事人信息</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="Person">
