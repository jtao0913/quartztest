						<xs:documentation>国际标准图书号</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="INO" type="string" minOccurs="0"/>
				<xs:element name="DOI" type="string" minOccurs="0"/>
				<xs:element name="PublisherID" minOccurs="0">
					<xs:annotation>
						<xs:documentation>出版商标示</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="VolumeNumber" minOccurs="0">
					<xs:annotation>
						<xs:documentation>劵期号</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="IssueNumber" minOccurs="0">
					<xs:annotation>
						<xs:documentation>发行号</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CopyRight" minOccurs="0">
					<xs:annotation>
						<xs:documentation>版权信息</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="RichContent" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SubName">
		<xs:annotation>
			<xs:documentation>相关人,引证模块</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="Person">
					<xs:attribute name="type">
						<xs:simpleType>
							<xs:restriction base="string">
								<xs:enumeration value="editor"/>
								<xs:enumeration value="translator"/>
								<xs:enumeration value="other-subname-type"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Series">
		<xs:annotation>
			<xs:documentation>引用丛书</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MonographicTitle" type="string" minOccurs="0"/>
				<xs:element name="MonographicNumber" type="string" minOccurs="0"/>
				<xs:element name="ISSN" type="string" minOccurs="0"/>
				<xs:element name="Text" type="Text" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ElectronicAccess">
		<xs:annotation>
			<xs:documentation>电子途径</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MediumCategory" type="string" minOccurs="0"/>
				<xs:element name="DOI" type="string" minOccurs="0"/>
				<xs:element name="URL" type="string" minOccurs="0"/>
				<xs:element name="HostTitle" type="string" minOccurs="0"/>
				<xs:element name="SearchDate" type="Date" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CustomerNumber">
		<xs:simpleType>
			<xs:restriction base="string">
				<xs:pattern value="[0-9]{5}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="DocumentInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DocumentID"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="sequence"/>
			<xs:attributeGroup ref="source"/>
			<xs:attributeGroup ref="language"/>
			<xs:attribute name="type">
				<xs:simpleType>
					<xs:restriction base="string">
						<xs:enumeration value="pub"/>
						<xs:enumeration value="app"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="isRepresentative"/>
			<xs:attribute name="documentStatus"/>
			<xs:attribute name="docID"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ReceivedAtIB" type="Date">
		<xs:annotation>
			<xs:documentation>国际局接收时间</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DocumentID">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Country" type="Country" minOccurs="0">
					<xs:annotation>
						<xs:documentation>国家</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Number" type="Number" minOccurs="0">
					<xs:annotation>
						<xs:documentation>号码</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Kind" minOccurs="0">
					<xs:annotation>
						<xs:documentation>类型</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="Kind">
								<xs:attribute name="source" type="string"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="Name" type="Name" minOccurs="0">
					<xs:annotation>
						<xs:documentation>名字</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Date" type="Date" minOccurs="0">
					<xs:annotation>
						<xs:documentation>日期</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="sequence"/>
			<xs:attributeGroup ref="language"/>
			<xs:attributeGroup ref="source"/>
			<xs:attribute name="type">
				<xs:simpleType>
					<xs:restriction base="string">
						<xs:enumeration value="pub"/>
						<xs:enumeration value="app"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="documentStatus"/>
		</xs:complexType>
	</xs:element>
	<xs:group name="IPC">
		<xs:annotation>
			<xs:documentation>IPC（R）基本组合</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Section" minOccurs="0">
				<xs:annotation>
					<xs:documentation>部</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="string">
						<xs:pattern value="[A-Z]"/>
						<xs:pattern value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MainClass" minOccurs="0">
				<xs:annotation>
					<xs:documentation>大类</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="string">
						<xs:pattern value="[0-9]{2}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SubClass" minOccurs="0">
				<xs:annotation>
					<xs:documentation>小类</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="string">
						<xs:pattern value="[A-Z]"/>
						<xs:pattern value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MainGroup" minOccurs="0">
				<xs:annotation>
					<xs:documentation>大组</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="string">
						<xs:pattern value="[0-9]+"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SubGroup" minOccurs="0">
				<xs:annotation>
					<xs:documentation>小组</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="string">
						<xs:pattern value="[0-9]+"/>
					</xs:restriction>
				</xs:simpleType>
