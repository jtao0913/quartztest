	<xs:element name="SequenceListBasis">
		<xs:annotation>
			<xs:documentation>检索报告模块，生物序列表基础</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Material">
					<xs:annotation>
						<xs:documentation>材料</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Type">
								<xs:annotation>
									<xs:documentation>材料类型</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="table" use="required">
										<xs:simpleType>
											<xs:restriction base="string">
												<xs:enumeration value="true"/>
												<xs:enumeration value="false"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="sequenceListing">
										<xs:simpleType>
											<xs:restriction base="string">
												<xs:enumeration value="true"/>
												<xs:enumeration value="false"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="Format">
								<xs:annotation>
									<xs:documentation>材料格式</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="paper" use="required">
										<xs:simpleType>
											<xs:restriction base="string">
												<xs:enumeration value="true"/>
												<xs:enumeration value="false"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="electronic">
										<xs:simpleType>
											<xs:restriction base="string">
												<xs:enumeration value="true"/>
												<xs:enumeration value="false"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="FilingTime">
								<xs:annotation>
									<xs:documentation>注册时间</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="inApplication" use="required">
										<xs:simpleType>
											<xs:restriction base="string">
												<xs:enumeration value="true"/>
												<xs:enumeration value="false"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="togetherWithApplicatio" use="required">
										<xs:simpleType>
											<xs:restriction base="string">
												<xs:enumeration value="true"/>
												<xs:enumeration value="false"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="furnishedForSearch" use="required">
										<xs:simpleType>
											<xs:restriction base="string">
												<xs:enumeration value="true"/>
												<xs:enumeration value="false"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="amendment">
										<xs:simpleType>
											<xs:restriction base="string">
												<xs:enumeration value="true"/>
												<xs:enumeration value="false"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Statement" type="string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>声明</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="InventionUnity">
		<xs:annotation>
			<xs:documentation>检索报告模块，发明的单一性</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Paragraph" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="SearchFees" minOccurs="0">
					<xs:annotation>
						<xs:documentation>检索费用信息</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Fee" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="string">
											<xs:attribute ref="sequence"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="srepProtest">
							<xs:simpleType>
								<xs:restriction base="string">
									<xs:enumeration value="true"/>
									<xs:enumeration value="false"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="SupplementaryUnityInvention" type="string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>补充的发明整体检索报告</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ProtestRemark" minOccurs="0">
					<xs:annotation>
						<xs:documentation>异议标记</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="string">
								<xs:attribute name="protestRemark">
									<xs:simpleType>
										<xs:restriction base="string">
											<xs:enumeration value="no-protest">
												<xs:annotation>
													<xs:documentation>没有需要缴纳额外检索费用的异议</xs:documentation>
												</xs:annotation>
											</xs:enumeration>
											<xs:enumeration value="protest-fee-paid">
												<xs:annotation>
													<xs:documentation>申请人异议所带来的额外检索费用，需要的话还要伴随异议费用的缴纳</xs:documentation>
												</xs:annotation>
											</xs:enumeration>
											<xs:enumeration value="protest-fee-not-paid">
												<xs:annotation>
													<xs:documentation>申请人异议所带来的额外检索费用，但必要的异议费用没有在邀请函所注明的时间期限内缴纳</xs:documentation>
												</xs:annotation>
											</xs:enumeration>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ClaimsNumber" type="xs:integer">
		<xs:annotation>
			<xs:documentation>检索报告模块，权利要求号码</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="SequenceLists">
		<xs:annotation>
			<xs:documentation>序列表</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="DocPage"/>
				<xs:element name="SequenceList">
					<xs:complexType>
						<xs:choice minOccurs="0">
							<xs:sequence>
								<xs:element ref="BibliographicData"/>
								<xs:element name="SequenceCount" type="xs:integer">
									<xs:annotation>
										<xs:documentation>序列个数</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="SoftVersion" type="string" minOccurs="0">
									<xs:annotation>
										<xs:documentation>软件版本</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="SequenceContent" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>序列内容</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="SequenceID" type="string">
												<xs:annotation>
													<xs:documentation>序列标示</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="SequenceLength" type="string" minOccurs="0">
												<xs:annotation>
													<xs:documentation>序列长度</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="SequenceType" type="string" minOccurs="0">
												<xs:annotation>
													<xs:documentation>序列类型</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="Organism" type="string" minOccurs="0">
												<xs:annotation>
													<xs:documentation>生物体名称</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="SequenceFeatureDetails" minOccurs="0">
												<xs:annotation>
													<xs:documentation>序列特征集合</xs:documentation>
												</xs:annotation>
												<xs:complexType mixed="true">
													<xs:sequence>
														<xs:element name="SequenceFeature" minOccurs="0" maxOccurs="unbounded">
															<xs:annotation>
																<xs:documentation>序列特征</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="SequenceKeywords" type="string" minOccurs="0">
																		<xs:annotation>
																			<xs:documentation>序列关键字</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="SequenceLocation" type="string" minOccurs="0">
																		<xs:annotation>
																			<xs:documentation>序列特征</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="OtherInformation" type="string" minOccurs="0">
																		<xs:annotation>
																			<xs:documentation>其他特征</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:sequence>
															</xs:complexType>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element name="SequencePublication" minOccurs="0">
												<xs:annotation>
													<xs:documentation>序列公开信息</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element ref="Author" minOccurs="0">
															<xs:annotation>
																<xs:documentation>作者</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element ref="Title" minOccurs="0"/>
														<xs:element name="JournalName" type="string" minOccurs="0">
															<xs:annotation>
																<xs:documentation>公开的杂志名称</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="VolumeNumber" type="string" minOccurs="0">
															<xs:annotation>
																<xs:documentation>卷号</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="Issue" type="string" minOccurs="0">
															<xs:annotation>
																<xs:documentation>发行信息</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element ref="PageRange" minOccurs="0">
															<xs:annotation>
																<xs:documentation>页码</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="Date" type="Date" minOccurs="0">
															<xs:annotation>
																<xs:documentation>日期</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="DatabaseAccessionNumber" type="string" minOccurs="0">
															<xs:annotation>
																<xs:documentation>数据库登记号</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="DatabaseEntryDate" type="Date" minOccurs="0">
															<xs:annotation>
																<xs:documentation>登入数据库日期</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element ref="PublicationInfo" minOccurs="0"/>
														<xs:element ref="ApplicationInfo" minOccurs="0"/>
														<xs:element name="RelevantResidues" type="string" minOccurs="0">
															<xs:annotation>
																<xs:documentation>酸基</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element name="SequenceDescription" type="Text">
												<xs:annotation>
													<xs:documentation>序列描述</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:choice maxOccurs="unbounded">
								<xs:element ref="Heading"/>
								<xs:element ref="Paragraph"/>
							</xs:choice>
						</xs:choice>
						<xs:attribute name="id" type="string"/>
						<xs:attributeGroup ref="language"/>
						<xs:attribute name="file"/>
						<xs:attribute name="carriers">
							<xs:simpleType>
								<xs:restriction base="xs:NMTOKEN">
									<xs:enumeration value="paper"/>
									<xs:enumeration value="internalElectronic"/>
									<xs:enumeration value="externalElectronic"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="sequenceFileType" type="string" fixed="ST.25"/>
						<xs:attribute name="electronicFormatType">
							<xs:simpleType>
								<xs:restriction base="xs:NMTOKEN">
									<xs:enumeration value="text"/>
									<xs:enumeration value="prj"/>
									<xs:enumeration value="psv"/>
									<xs:enumeration value="other"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="partOfApplication">
							<xs:simpleType>
								<xs:restriction base="xs:NMTOKEN">
									<xs:enumeration value="yes"/>
									<xs:enumeration value="no"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:attribute name="id" type="string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="CitingAndCited">
		<xs:annotation>
			<xs:documentation>引证和被引证</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PublicationInfo" maxOccurs="unbounded"/>
				<xs:element ref="ApplicationInfo" maxOccurs="unbounded"/>
				<xs:element ref="ReferencesCited" minOccurs="0"/>
				<xs:element ref="ForwardCit" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="sequence"/>
			<xs:attributeGroup ref="source"/>
			<xs:attributeGroup ref="language"/>
			<xs:attribute ref="status"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="LegalDocument">
		<xs:annotation>
			<xs:documentation>法律状态</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PublicationInfo" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ApplicationInfo" maxOccurs="unbounded"/>
				<xs:element ref="Legal" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="sequence" use="required"/>
			<xs:attribute name="patentType" use="required">
				<xs:annotation>
					<xs:documentation>专利类型</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="string">
						<xs:enumeration value="invention"/>
						<xs:enumeration value="utility model"/>
						<xs:enumeration value="design"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="status"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="OperationalDocuments">
		<xs:annotation>
			<xs:documentation>运营信息</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ApplicationInfo" maxOccurs="unbounded"/>
				<xs:element name="AssignmentInfo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>权利转移</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CurrentAssignee" minOccurs="0">
								<xs:annotation>
									<xs:documentation>当前权利人</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Assignee" maxOccurs="unbounded">
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="Person">
														<xs:attribute ref="organizationType"/>
														<xs:attribute ref="active"/>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="AssignmentRecord">
								<xs:annotation>
									<xs:documentation>转移发生记录</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Assignment" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="AssignmentDetail">
														<xs:annotation>
															<xs:documentation>转移信息</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="EffectiveDate" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>生效日</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="AssignmentKind" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>“专利权” or “申请权”</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="Kind">
																	<xs:annotation>
																		<xs:documentation>“转移” or “变更”</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="Assignors" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>转移前权利人</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="Assignor" maxOccurs="unbounded">
																				<xs:complexType>
																					<xs:complexContent>
																						<xs:extension base="Person">
																							<xs:attribute ref="organizationType"/>
																							<xs:attribute ref="active"/>
																						</xs:extension>
																					</xs:complexContent>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="Assignees" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>转以后权利人</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="Assignee" maxOccurs="unbounded">
																				<xs:complexType>
																					<xs:complexContent>
																						<xs:extension base="Person">
																							<xs:attribute ref="organizationType"/>
																							<xs:attribute ref="active"/>
																						</xs:extension>
																					</xs:complexContent>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element ref="Legal"/>
												</xs:sequence>
												<xs:attribute ref="sequence"/>
												<xs:attribute name="eventDate">
													<xs:annotation>
														<xs:documentation>事件公开时间</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PledgeInfo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>质押保全</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CurrentPledgees" minOccurs="0">
								<xs:annotation>
									<xs:documentation>当前质权人</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Pledgee" maxOccurs="unbounded">
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="Person">
														<xs:attribute ref="organizationType"/>
														<xs:attribute ref="active"/>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="PledgeRecord">
								<xs:annotation>
									<xs:documentation>质押保全发生记录</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Pledge" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="PledgeDetail">
														<xs:complexType>
															<xs:sequence>
																<xs:choice>
																	<xs:element name="EffectiveDate">
																		<xs:annotation>
																			<xs:documentation>生效日</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="ChangeDate">
																		<xs:annotation>
																			<xs:documentation>变更日</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="ExemptionDate">
																		<xs:annotation>
																			<xs:documentation>接触日</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:choice>
																<xs:element name="PledgeAndPreservationKind" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>“质押” or “保全”</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ContractStatus">
																	<xs:annotation>
																		<xs:documentation>“生效” or “变更”  or “注销”</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ContractRegistrationNumber" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>合同注册号</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="Pledgors" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>出质人</xs:documentation>
