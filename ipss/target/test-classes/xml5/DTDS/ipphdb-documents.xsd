		</xs:complexType>
	</xs:element>
	<xs:element name="Regional">
		<xs:annotation>
			<xs:documentation>PCT指定国标签模块</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Region" minOccurs="0"/>
				<xs:sequence maxOccurs="unbounded">
					<xs:element name="Country" type="Country"/>
					<xs:element name="ProtectionRequest" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="KindOfProtection" type="string"/>
								<xs:element ref="DocumentID" minOccurs="0"/>
							</xs:sequence>
							<xs:attribute name="subset"/>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
				<xs:element name="AnyOtherStates" type="Text" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="National">
		<xs:annotation>
			<xs:documentation>PCT指定国标签模块</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="Country" type="Country"/>
				<xs:element name="ProtectionRequest" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="KindOfProtection" type="string"/>
							<xs:element ref="DocumentID" minOccurs="0"/>
						</xs:sequence>
						<xs:attribute name="subset"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Citation">
		<xs:annotation>
			<xs:documentation>引证模块</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice minOccurs="0">
					<xs:element ref="PatentCitation"/>
					<xs:element ref="NPLCitation" maxOccurs="unbounded"/>
				</xs:choice>
				<xs:element ref="Location" minOccurs="0" maxOccurs="unbounded"/>
