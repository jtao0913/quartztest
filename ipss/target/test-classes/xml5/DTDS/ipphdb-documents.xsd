													<xs:sequence>
														<xs:element ref="RelatedDocument"/>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:choice>
				<xs:element name="Correction">
					<xs:annotation>
						<xs:documentation>专利更正信息,ST.32-B665</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DocumentCorrected">
								<xs:annotation>
									<xs:documentation>被更正的文档，ST.32-B666</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="DocumentID" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="CorrectionType" type="string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>更正类型,ST.32-B667</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element ref="GazetteReference" minOccurs="0"/>
							<xs:element name="Text" type="Text" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="UtilityModelBasis">
					<xs:annotation>
						<xs:documentation>实用新型基于的文档,ST.32-B670</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="RelatedDocument"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PreviouslyFiledApplication">
					<xs:annotation>
						<xs:documentation>在先提交的申请</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="RelatedDocument"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ConcurrentApplication">
					<xs:annotation>
						<xs:documentation>同日申请</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="DocumentInfo" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="OtherRelatedDocument">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="RelatedDocument"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="Parties">
		<xs:annotation>
			<xs:documentation>专利有关当事人，INID70</xs:documentation>
