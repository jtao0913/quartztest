				</xs:element>
				<xs:element name="HostTitle" type="Text" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>数据库或者文档集标题</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="HostNumber" type="string" minOccurs="0">
