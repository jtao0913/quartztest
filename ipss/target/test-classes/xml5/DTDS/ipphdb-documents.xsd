							<xs:extension base="Person">
								<xs:sequence>
									<xs:element name="ApplicantCode" type="string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>申请人机构代码</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="USRights" minOccurs="0">
										<xs:annotation>
											<xs:documentation>该人是已故的、精神失常的或无法定能力的发明人的权利的继承人。由美国指定的。</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="string">
													<xs:attribute name="toDeadInventor"/>
													<xs:attribute name="kind"/>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element ref="DesignationOfStates" minOccurs="0"/>
									<xs:element ref="DesignatedStatesAsInventor" minOccurs="0"/>
									<xs:element name="Text" type="Text" minOccurs="0"/>
								</xs:sequence>
								<xs:attribute ref="organizationType"/>
								<xs:attribute ref="designation"/>
								<xs:attribute name="applicantType">
									<xs:simpleType>
										<xs:restriction base="string">
											<xs:enumeration value="applicant"/>
											<xs:enumeration value="applicant-inventor"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute name="representative"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Inventors">
		<xs:annotation>
			<xs:documentation>发明人，INID72</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Inventor" minOccurs="0" maxOccurs="unbounded">
