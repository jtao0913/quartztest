	<xs:element name="AppealDecisionDocument">
		<xs:annotation>
			<xs:documentation>复审决定信息</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AppealCaseInformation"/>
				<xs:element ref="DecisionDetail"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="sequence" use="required"/>
			<xs:attribute ref="lang"/>
			<xs:attribute name="fileReferenceId" type="string">
				<xs:annotation>
					<xs:documentation>文献中涉及的文件的编码</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="correctionCode" type="string">
				<xs:annotation>
					<xs:documentation>修正码</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="datePublication" type="Date">
				<xs:annotation>
					<xs:documentation>文献公布/公告日期</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute ref="status"/>
			<xs:attribute ref="appealType" use="required"/>
			<xs:attribute ref="decisionType" use="required"/>
			<xs:attribute name="effective">
				<xs:annotation>
					<xs:documentation>指出该决定是否生效</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="yes"/>
						<xs:enumeration value="no"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="patentDataResourceID" type="string" use="optional">
				<xs:annotation>
					<xs:documentation>专利数据资源标识码</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AppealCaseInformation">
		<xs:annotation>
			<xs:documentation>复审决定基本信息</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="DecisionNumber" type="Number">
					<xs:annotation>
						<xs:documentation>复审或无效宣告的决定号</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DecisionDate" type="Date">
					<xs:annotation>
						<xs:documentation>决定日期</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="PatentInformation" minOccurs="0"/>
				<xs:element name="AppealCaseParties">
					<xs:annotation>
						<xs:documentation>复审决定相关人信息</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:choice minOccurs="0">
								<xs:element name="Complainant" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>复审、撤销复审或无效宣告请求的相关人信息</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="Person"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="Appellant" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>无效宣告请求人或撤销复审请求人的基本信息</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="Person"/>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
							</xs:choice>
							<xs:element name="Appellee" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>无效宣告被请求人或撤销复审被请求人的基本信息</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="Person"/>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="Agents" minOccurs="0"/>
				<xs:element ref="Examiners"/>
				<xs:element ref="PublicationInformation" minOccurs="0"/>
				<xs:element ref="AccessoryDetails" minOccurs="0"/>
				<xs:element name="DecisionAbstract">
					<xs:annotation>
						<xs:documentation>审查决定的摘要</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="LegalBasis" minOccurs="0"/>
							<xs:element name="DecisionMainPointDetails">
								<xs:annotation>
									<xs:documentation>所有审查决定的决定要点</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="DecisionMainPoint" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence maxOccurs="unbounded">
													<xs:element ref="Paragraph"/>
												</xs:sequence>
												<xs:attribute name="id" type="string"/>
												<xs:attribute ref="sequence"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="Abstract" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Keywords" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="DecisionDrawings" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DecisionDetail">
		<xs:annotation>
			<xs:documentation>审查决定的决定正文</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="DocPage" maxOccurs="unbounded"/>
				<xs:sequence>
					<xs:element name="DecisionCaseIssue" minOccurs="0">
						<xs:annotation>
							<xs:documentation>案由</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:choice maxOccurs="unbounded">
								<xs:element ref="Heading"/>
								<xs:element ref="Paragraph"/>
							</xs:choice>
						</xs:complexType>
					</xs:element>
					<xs:element name="Reasoning" minOccurs="0">
						<xs:annotation>
							<xs:documentation>决定的理由</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:choice maxOccurs="unbounded">
								<xs:element ref="Heading"/>
								<xs:element ref="Paragraph"/>
							</xs:choice>
						</xs:complexType>
					</xs:element>
					<xs:element name="FinalDecision" minOccurs="0">
						<xs:annotation>
							<xs:documentation>决定</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:choice maxOccurs="unbounded">
								<xs:element ref="Heading"/>
								<xs:element ref="Paragraph"/>
							</xs:choice>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="PatentInformation">
		<xs:annotation>
			<xs:documentation>相关专利信息,与Verdict共享</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ApplicationInfo" maxOccurs="unbounded"/>
				<xs:element ref="Applicants" minOccurs="0"/>
				<xs:element ref="PublicationInfo" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Assignees" minOccurs="0"/>
				<xs:element ref="Title" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ClassificationIPC" minOccurs="0"/>
				<xs:element ref="ClassificationsIPCR" minOccurs="0"/>
				<xs:element ref="ClassificationNational" minOccurs="0"/>
				<xs:element ref="ClassificationsLocarno" minOccurs="0"/>
				<xs:element ref="PriorityClaimDetails" minOccurs="0"/>
				<xs:element name="InitialApplicationNumber" type="string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>原案申请号</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute ref="appType"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="PublicationInformation">
		<xs:annotation>
			<xs:documentation>公告信息</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DocumentID" minOccurs="0"/>
				<xs:element name="PublicationVolume" type="string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>公告卷期</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AccessoryDetails">
		<xs:annotation>
			<xs:documentation>复审决定基本信息的模块：审查决定的附件，与Paragraph共享</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="Accessory" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Accessory">
		<xs:annotation>
			<xs:documentation>复审决定基本信息的模块：审查决定的附件，与Paragraph共享</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="B"/>
				<xs:element ref="I"/>
				<xs:element ref="U"/>
				<xs:element ref="O"/>
				<xs:element ref="Sup"/>
				<xs:element ref="Sub"/>
				<xs:element ref="Smallcaps"/>
				<xs:element ref="Br"/>
				<xs:element ref="FigureRef"/>
				<xs:element ref="CrossRef"/>
				<xs:element ref="PatentCitation"/>
				<xs:sequence>
					<xs:element ref="SearchField" minOccurs="0"/>
					<xs:element ref="NPLCitation"/>
				</xs:sequence>
				<xs:element ref="BiologicalDeposit"/>
				<xs:element ref="LegalBasis"/>
				<xs:element ref="EvidenceRef"/>
			</xs:choice>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="sequence"/>
			<xs:attribute name="docName" type="string"/>
			<xs:attribute name="fileName" type="string" use="required"/>
			<xs:attribute name="size" type="string"/>
			<xs:attribute name="dateProduced" type="Date"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="LegalBasis">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="LawReference"/>
				<xs:element ref="NoticeReference"/>
				<xs:element ref="RuleReference"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="LawReference">
		<xs:annotation>
			<xs:documentation>法律条款的引用</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="sequence">
				<xs:annotation>
					<xs:documentation>每个引用法条的次序号，取值为自然数</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="lawID" type="string" use="required">
				<xs:annotation>
					<xs:documentation>法律或法院判决书的类别.如：专利法、行政判决书</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="lawVersion" type="string">
				<xs:annotation>
					<xs:documentation>引用法律的版本</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="articleID" type="string">
				<xs:annotation>
					<xs:documentation>引用的特定的法律文本的条目.依照原文，如第23条标注为"23"；一中行初字第755号标注为"一中行初字第755号"</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="termID" type="string">
				<xs:annotation>
					<xs:documentation>引用法条中的特定款项.依照原文，如第2款标注为"2" </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
