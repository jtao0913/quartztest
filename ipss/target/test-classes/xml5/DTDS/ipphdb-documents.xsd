		<xs:annotation>
			<xs:documentation>斜体/Italic format</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="B"/>
				<xs:element ref="I"/>
				<xs:element ref="U"/>
				<xs:element ref="O"/>
				<xs:element ref="Sub"/>
				<xs:element ref="Sup"/>
				<xs:element ref="Del"/>
				<xs:element ref="Ins"/>
				<xs:element ref="Smallcaps"/>
				<xs:element ref="Br"/>
				<xs:element ref="PageBegin"/>
				<xs:element ref="Image"/>
				<xs:element ref="Math"/>
				<xs:element ref="Chemistry"/>
				<xs:element ref="Table"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="U">
		<xs:annotation>
			<xs:documentation>下划线/Underscore format</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="B"/>
				<xs:element ref="I"/>
				<xs:element ref="U"/>
				<xs:element ref="O"/>
				<xs:element ref="Sub"/>
				<xs:element ref="Sup"/>
				<xs:element ref="Del"/>
				<xs:element ref="Ins"/>
				<xs:element ref="Smallcaps"/>
				<xs:element ref="Br"/>
				<xs:element ref="PageBegin"/>
				<xs:element ref="Image"/>
				<xs:element ref="Math"/>
				<xs:element ref="Chemistry"/>
				<xs:element ref="Table"/>
			</xs:choice>
			<xs:attribute name="style" default="single">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="dash"/>
						<xs:enumeration value="single"/>
						<xs:enumeration value="dots"/>
						<xs:enumeration value="double"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="O">
		<xs:annotation>
			<xs:documentation>上划线/Overscore format</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="B"/>
				<xs:element ref="I"/>
				<xs:element ref="U"/>
				<xs:element ref="O"/>
				<xs:element ref="Sub"/>
				<xs:element ref="Sup"/>
				<xs:element ref="Del"/>
				<xs:element ref="Ins"/>
				<xs:element ref="Smallcaps"/>
				<xs:element ref="Br"/>
				<xs:element ref="PageBegin"/>
				<xs:element ref="Image"/>
				<xs:element ref="Math"/>
				<xs:element ref="Chemistry"/>
				<xs:element ref="Table"/>
			</xs:choice>
			<xs:attribute name="style">
				<xs:simpleType>
					<xs:restriction base="string">
						<xs:enumeration value="dash"/>
						<xs:enumeration value="single"/>
						<xs:enumeration value="leftarrow"/>
						<xs:enumeration value="rightarrow"/>
						<xs:enumeration value="leftrightarrow"/>
						<xs:enumeration value="dots"/>
						<xs:enumeration value="double"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Sub">
		<xs:annotation>
			<xs:documentation>下标/Superscripted format</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="B"/>
				<xs:element ref="I"/>
				<xs:element ref="U"/>
				<xs:element ref="O"/>
				<xs:element ref="Sub"/>
				<xs:element ref="Sup"/>
				<xs:element ref="Del"/>
				<xs:element ref="Ins"/>
				<xs:element ref="Smallcaps"/>
				<xs:element ref="Br"/>
				<xs:element ref="PageBegin"/>
				<xs:element ref="Image"/>
				<xs:element ref="Math"/>
				<xs:element ref="Chemistry"/>
				<xs:element ref="Table"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="Sup">
		<xs:annotation>
			<xs:documentation>上标/Subscripted format</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="B"/>
				<xs:element ref="I"/>
				<xs:element ref="U"/>
				<xs:element ref="O"/>
				<xs:element ref="Sub"/>
				<xs:element ref="Sup"/>
				<xs:element ref="Del"/>
				<xs:element ref="Ins"/>
				<xs:element ref="Smallcaps"/>
				<xs:element ref="Br"/>
				<xs:element ref="PageBegin"/>
				<xs:element ref="Image"/>
				<xs:element ref="Math"/>
				<xs:element ref="Chemistry"/>
				<xs:element ref="Table"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="Del">
		<xs:annotation>
			<xs:documentation>删除/Strikethrough format</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="B"/>
				<xs:element ref="I"/>
				<xs:element ref="U"/>
				<xs:element ref="O"/>
				<xs:element ref="Sub"/>
				<xs:element ref="Sup"/>
				<xs:element ref="Del"/>
				<xs:element ref="Ins"/>
				<xs:element ref="Smallcaps"/>
				<xs:element ref="Br"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="Ins">
		<xs:annotation>
			<xs:documentation>插入/Inserted format</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="B"/>
				<xs:element ref="I"/>
				<xs:element ref="U"/>
				<xs:element ref="O"/>
				<xs:element ref="Sub"/>
				<xs:element ref="Sup"/>
				<xs:element ref="Del"/>
				<xs:element ref="Ins"/>
				<xs:element ref="Smallcaps"/>
				<xs:element ref="Br"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="Smallcaps">
		<xs:annotation>
			<xs:documentation>小体大写/Smallcaps format</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="B"/>
				<xs:element ref="I"/>
				<xs:element ref="U"/>
				<xs:element ref="O"/>
				<xs:element ref="Sub"/>
				<xs:element ref="Sup"/>
				<xs:element ref="Del"/>
				<xs:element ref="Ins"/>
				<xs:element ref="Smallcaps"/>
				<xs:element ref="Br"/>
				<xs:element ref="PageBegin"/>
				<xs:element ref="Image"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="Br">
		<xs:annotation>
			<xs:documentation>回车/Break format</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="B"/>
				<xs:element ref="I"/>
				<xs:element ref="U"/>
				<xs:element ref="O"/>
				<xs:element ref="Sub"/>
				<xs:element ref="Sup"/>
				<xs:element ref="Del"/>
				<xs:element ref="Ins"/>
				<xs:element ref="Smallcaps"/>
				<xs:element ref="Br"/>
			</xs:choice>
			<xs:attribute name="rnum"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="PageBegin">
		<xs:annotation>
			<xs:documentation>页的开始</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="string">
					<xs:attribute name="pnum"/>
					<xs:attribute name="rnum"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="DocPage">
		<xs:annotation>
			<xs:documentation>文档页</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="id" type="string"/>
			<xs:attribute name="he" type="string"/>
			<xs:attribute name="wi" type="string"/>
			<xs:attribute name="type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="tif"/>
						<xs:enumeration value="gif"/>
						<xs:enumeration value="pdf"/>
						<xs:enumeration value="jpg"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="alt" type="string"/>
			<xs:attribute name="pp" type="string"/>
			<xs:attribute name="ppf" type="string"/>
			<xs:attribute name="ppl" type="string"/>
			<xs:attribute name="file" type="string" use="required"/>
			<xs:attribute name="ocr" type="string"/>
			<xs:attribute name="color">
				<xs:simpleType>
					<xs:restriction base="string">
						<xs:enumeration value="color"/>
						<xs:enumeration value="bw"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="orientation" default="portrait">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="portrait"/>
						<xs:enumeration value="landscape"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Figure">
		<xs:annotation>
			<xs:documentation>图形模块</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="Image"/>
				<xs:element ref="DrawingText" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute name="number" type="string"/>
			<xs:attribute name="figureLabels" type="string"/>
			<xs:attribute name="pageEnd" type="string" use="optional">
				<xs:annotation>
					<xs:documentation>仅当当前图位于某一页的结束处时使用该属性，属性值为该图所结束的页码号</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Image">
		<xs:annotation>
			<xs:documentation>图像模块</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ImageText" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Paragraph"/>
						</xs:sequence>
						<xs:attribute ref="sequence"/>
						<xs:attribute name="imageTextType">
							<xs:simpleType>
								<xs:restriction base="string">
									<xs:enumeration value="imageCaption">
										<xs:annotation>
											<xs:documentation>图像标题</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
									<xs:enumeration value="reference sign">
										<xs:annotation>
											<xs:documentation>附图标记</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
									<xs:enumeration value="reference sign coordinates">
										<xs:annotation>
											<xs:documentation>附图标记的起始坐标</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute ref="lang"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute name="he" type="string" use="optional"/>
			<xs:attribute name="wi" type="string" use="optional"/>
			<xs:attribute name="top" type="string"/>
			<xs:attribute name="left" type="string"/>
			<xs:attribute name="file" type="string" use="required">
				<xs:annotation>
					<xs:documentation>若图像和PDF在同一文件夹下，则仅指文件名；反之若不在同一文件夹下，需要指明绝对路径和文件名</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="alt" type="string"/>
			<xs:attribute name="imgContent" default="drawing">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="photograph"/>
						<xs:enumeration value="dna"/>
						<xs:enumeration value="drawing"/>
						<xs:enumeration value="character"/>
						<xs:enumeration value="chem"/>
						<xs:enumeration value="undefined"/>
						<xs:enumeration value="flowchart"/>
						<xs:enumeration value="table"/>
						<xs:enumeration value="graph"/>
						<xs:enumeration value="program-listing"/>
						<xs:enumeration value="math"/>
						<xs:enumeration value="document"/>
						<xs:enumeration value="specimen"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="imgFormat">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="GIF"/>
						<xs:enumeration value="JPEG"/>
						<xs:enumeration value="PDF"/>
						<xs:enumeration value="IMG"/>
						<xs:enumeration value="TIFF"/>
						<xs:enumeration value="DWF"/>
						<xs:enumeration value="DWG"/>
						<xs:enumeration value="IGES"/>
						<xs:enumeration value="IGS"/>
						<xs:enumeration value="3DS"/>
						<xs:enumeration value="WMV"/>
						<xs:enumeration value="3DM"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="resolution" type="string">
				<xs:annotation>
					<xs:documentation>图像分辨率，分辨率取值应在150DPI和400DPI之间，推荐分辨率300DPI</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="orientation" default="portrait">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="landscape"/>
						<xs:enumeration value="portrait"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="inline" default="no">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="yes"/>
						<xs:enumeration value="no"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="embedded" use="optional">
				<xs:annotation>
					<xs:documentation>该图像是否是嵌入于文档中的，是的话取值为"true"，否则为"false"</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="true"/>
						<xs:enumeration value="false"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="DrawingText">
		<xs:annotation>
			<xs:documentation>图像中文字的代码化内容</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Paragraph"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExternalFileRef">
		<xs:annotation>
			<xs:documentation>外部独立文件</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="id" type="string"/>
					<xs:attribute name="idRef" type="string"/>
					<xs:attribute name="file" type="string" use="required"/>
					<xs:attribute name="lang" type="string"/>
					<xs:attribute name="externalType">
						<xs:annotation>
							<xs:documentation>文件内容类型</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="status" type="string"/>
					<xs:attribute name="carriers">
						<xs:simpleType>
							<xs:restriction base="xs:token">
								<xs:enumeration value="paper"/>
								<xs:enumeration value="tape"/>
								<xs:enumeration value="electronic"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="ObjectChange">
		<xs:annotation>
			<xs:documentation>AmendedClaims模块</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="Title"/>
			<xs:element ref="Heading"/>
			<xs:element ref="Paragraph"/>
			<xs:element ref="DocPage"/>
			<xs:element ref="Claim"/>
			<xs:element ref="Figure"/>
		</xs:choice>
		<xs:attribute name="number"/>
		<xs:attribute name="location"/>
	</xs:complexType>
	<xs:element name="Preservation">
		<xs:annotation>
			<xs:documentation>保持原有格式</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="string">
					<xs:attribute name="id" type="string"/>
					<xs:attribute name="space">
						<xs:simpleType>
							<xs:restriction base="string">
								<xs:enumeration value="default"/>
								<xs:enumeration value="preserve"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="listingType">
						<xs:simpleType>
							<xs:restriction base="string">
								<xs:enumeration value="program-listing"/>
								<xs:enumeration value="sequence"/>
								<xs:enumeration value="other"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="DefinitionList">
		<xs:annotation>
			<xs:documentation>定义术语列表</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="DefinedTerm" type="Paragraph" minOccurs="0"/>
				<xs:element name="DefinitionDescription" type="Paragraph" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute name="tsize" type="string"/>
			<xs:attribute name="compact">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="compact"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="markushList"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="OrderedList">
		<xs:annotation>
			<xs:documentation>有序列表</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ListItem" type="Paragraph" maxOccurs="unbounded"/>
				<xs:element ref="Paragraph" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute name="listStyle" type="string"/>
			<xs:attribute name="compact">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="compact"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="UnorderedList">
		<xs:annotation>
			<xs:documentation>无序列表</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ListItem" type="Paragraph" maxOccurs="unbounded"/>
				<xs:element ref="Paragraph" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute name="listStyle" default="bullet">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="dash"/>
						<xs:enumeration value="none"/>
						<xs:enumeration value="bullet"/>
						<xs:enumeration value="dots"/>
						<xs:enumeration value="double"/>
						<xs:enumeration value="single"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="compact">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="compact"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FigureRef">
		<xs:annotation>
			<xs:documentation>引用图形</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="RichText">
					<xs:attribute name="number"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CrossRef" type="RichText">
		<xs:annotation>
			<xs:documentation>引用id属性的元素</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Math">
		<xs:annotation>
			<xs:documentation>数学式模块</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="Image"/>
				<xs:element name="FormulaText">
					<xs:annotation>
						<xs:documentation>简单格式描述数据式</xs:documentation>
					</xs:annotation>
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:element ref="B"/>
							<xs:element ref="I"/>
							<xs:element ref="U"/>
							<xs:element ref="O"/>
							<xs:element ref="Sup"/>
							<xs:element ref="Sub"/>
							<xs:element ref="Smallcaps"/>
							<xs:element ref="Image"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element name="MathFormula">
					<xs:annotation>
						<xs:documentation>数学公式，遵循mathml3加工标准</xs:documentation>
					</xs:annotation>
					<xs:complexType mixed="true">
						<xs:sequence>
							<xs:element ref="M:math"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="ExternalFileRef"/>
				<xs:element name="MathText" type="Text">
					<xs:annotation>
						<xs:documentation>以CDATA的方式存储数学公式</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:attribute name="id" type="string"/>
			<xs:attribute name="number" type="string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Table">
		<xs:annotation>
			<xs:documentation>表格模块</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="Image"/>
				<xs:element name="TableFormula">
					<xs:annotation>
						<xs:documentation>以标签形式存储表格</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:element name="Title" type="RichText" minOccurs="0"/>
							<xs:element name="TableGroup" maxOccurs="unbounded">
								<xs:complexType mixed="true">
									<xs:sequence>
										<xs:element name="Colspec" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:attribute name="colNumber"/>
												<xs:attribute name="colName"/>
												<xs:attribute name="colWidth"/>
												<xs:attribute name="colSep"/>
												<xs:attribute name="rowSep"/>
												<xs:attribute name="align"/>
												<xs:attribute name="char"/>
												<xs:attribute name="charOff"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="Thead" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="Row" maxOccurs="unbounded"/>
												</xs:sequence>
												<xs:attribute name="valign"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="Tbody" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="Row" minOccurs="0" maxOccurs="unbounded"/>
												</xs:sequence>
												<xs:attribute name="valign"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="cols" use="required"/>
									<xs:attribute name="colSep"/>
									<xs:attribute name="rowSep"/>
									<xs:attribute name="align"/>
								</xs:complexType>
							</xs:element>
						</xs:choice>
						<xs:attribute name="id" type="string"/>
					</xs:complexType>
				</xs:element>
				<xs:element ref="ExternalFileRef"/>
				<xs:element name="TableText" type="Text">
					<xs:annotation>
						<xs:documentation>以CDATA的方式存储表格</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:attribute name="id" type="string"/>
			<xs:attribute name="number" type="string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Chemistry">
		<xs:annotation>
			<xs:documentation>化学式模块</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="Image"/>
				<xs:element name="ChemistryFormula">
					<xs:annotation>
						<xs:documentation>化学式公式</xs:documentation>
					</xs:annotation>
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:element ref="B"/>
							<xs:element ref="I"/>
							<xs:element ref="U"/>
							<xs:element ref="O"/>
							<xs:element ref="Sup"/>
							<xs:element ref="Sub"/>
							<xs:element ref="Smallcaps"/>
							<xs:element ref="Br"/>
							<xs:element ref="Image"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element ref="ExternalFileRef"/>
				<xs:element name="ChemistryText" type="Text">
					<xs:annotation>
						<xs:documentation>以CDATA的方式存储表格</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:attribute name="id" type="string"/>
			<xs:attribute name="number" type="string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="TechnicalProblem">
		<xs:annotation>
			<xs:documentation>发明针对的技术问题</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="Heading"/>
				<xs:element ref="Paragraph"/>
			</xs:choice>
			<xs:attribute name="id" type="string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="TechnicalSolution">
		<xs:annotation>
			<xs:documentation>发明的技术解决方案</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="Heading"/>
				<xs:element ref="Paragraph"/>
			</xs:choice>
			<xs:attribute name="id" type="string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AdvantageousEffects">
		<xs:annotation>
			<xs:documentation>发明的先进性</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="Heading"/>
				<xs:element ref="Paragraph"/>
			</xs:choice>
			<xs:attribute name="id" type="string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Row">
		<xs:annotation>
			<xs:documentation>表格中的单元格</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="entry" maxOccurs="unbounded">
					<xs:complexType mixed="true">
						<xs:choice minOccurs="0" maxOccurs="unbounded">
							<xs:element ref="B"/>
							<xs:element ref="I"/>
							<xs:element ref="U"/>
							<xs:element ref="O"/>
							<xs:element ref="Sup"/>
							<xs:element ref="Sub"/>
							<xs:element ref="Smallcaps"/>
							<xs:element ref="Br"/>
							<xs:element ref="Preservation"/>
							<xs:element ref="DefinitionList"/>
							<xs:element ref="PatentCitation"/>
							<xs:element ref="NPLCitation"/>
							<xs:element ref="BiologicalDeposit"/>
							<xs:element ref="CrossRef"/>
							<xs:element ref="FigureRef"/>
							<xs:element ref="Image"/>
							<xs:element ref="Chemistry"/>
							<xs:element ref="Math"/>
						</xs:choice>
						<xs:attribute name="id"/>
						<xs:attribute name="colName"/>
						<xs:attribute name="nameStart"/>
						<xs:attribute name="nameEnd"/>
						<xs:attribute name="moreRows"/>
						<xs:attribute name="colSep"/>
						<xs:attribute name="rowSep"/>
						<xs:attribute name="align"/>
						<xs:attribute name="char"/>
						<xs:attribute name="charOff"/>
						<xs:attribute name="valign"/>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:attribute name="rowSep"/>
			<xs:attribute name="valign"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Legal">
		<xs:annotation>
			<xs:documentation>法律状态模块</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="LegalPublicationDate" type="Date">
					<xs:annotation>
						<xs:documentation>公开日</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="LegalCode" type="string">
					<xs:annotation>
						<xs:documentation>法律状态代码</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="LegalDescription" type="Text" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>法律状态事项描述</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="LegalDescriptionDetail" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>法律状态事项描述细节</xs:documentation>
					</xs:annotation>
					<xs:complexType mixed="true">
						<xs:sequence>
							<xs:element name="Title" type="string" minOccurs="0"/>
							<xs:element name="Items" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Item" maxOccurs="unbounded">
											<xs:complexType mixed="true">
												<xs:complexContent>
													<xs:extension base="RichText">
														<xs:sequence>
															<xs:element name="Contents" minOccurs="0" maxOccurs="unbounded">
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="Content" maxOccurs="unbounded">
																			<xs:complexType>
																				<xs:simpleContent>
																					<xs:extension base="string">
																						<xs:attribute name="name"/>
																					</xs:extension>
																				</xs:simpleContent>
																			</xs:complexType>
																		</xs:element>
																	</xs:sequence>
																	<xs:attribute ref="sequence"/>
																</xs:complexType>
															</xs:element>
														</xs:sequence>
														<xs:attribute ref="sequence"/>
														<xs:attribute name="name" type="string"/>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="id" type="string"/>
						<xs:attributeGroup ref="source"/>
						<xs:attributeGroup ref="language"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="sequence"/>
			<xs:attributeGroup ref="language"/>
			<xs:attribute name="impact">
				<xs:annotation>
					<xs:documentation>10=有权 20=无权 01=无影响</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="string">
						<xs:enumeration value="10"/>
						<xs:enumeration value="20"/>
						<xs:enumeration value="01"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:attribute name="status">
		<xs:simpleType>
			<xs:restriction base="string">
				<xs:enumeration value="create"/>
				<xs:enumeration value="update"/>
				<xs:enumeration value="delete"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="quantity">
		<xs:annotation>
			<xs:documentation>数量</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:integer">
				<xs:minInclusive value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="year">
		<xs:annotation>
			<xs:documentation>年份</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="string">
				<xs:pattern value="(1[8-9][0-9][0-9]|200[0-9]|201[0-6])"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="sequence">
		<xs:annotation>
			<xs:documentation>序号</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:integer">
				<xs:minInclusive value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="unit">
		<xs:annotation>
			<xs:documentation>单位/measure unit</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="string"/>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="lang">
		<xs:annotation>
			<xs:documentation>语种</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="string">
				<xs:pattern value="[a-z]{2}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="format">
		<xs:annotation>
			<xs:documentation>翻译方式</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="string">
				<xs:enumeration value="original"/>
				<xs:enumeration value="translation"/>
				<xs:enumeration value="integration"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="organizationType">
		<xs:simpleType>
			<xs:restriction base="string">
				<xs:enumeration value="UC"/>
				<xs:enumeration value="SRI"/>
				<xs:enumeration value="IME"/>
				<xs:enumeration value="OT"/>
				<xs:enumeration value="PE"/>
				<xs:enumeration value="OTH"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="active">
		<xs:annotation>
			<xs:documentation>活性标识</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="string">
				<xs:enumeration value="yes"/>
				<xs:enumeration value="no"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="designation">
		<xs:simpleType>
			<xs:restriction base="string">
				<xs:enumeration value="all"/>
				<xs:enumeration value="all-except-us"/>
				<xs:enumeration value="us-only"/>
				<xs:enumeration value="as-indicated"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="appType">
		<xs:annotation>
			<xs:documentation>申请类型</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:token">
				<xs:enumeration value="patent">
					<xs:annotation>
						<xs:documentation>专利</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="utility">
					<xs:annotation>
						<xs:documentation>新型</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="design">
					<xs:annotation>
						<xs:documentation>外观</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="appealType">
		<xs:simpleType>
			<xs:restriction base="xs:token">
				<xs:enumeration value="reexamination">
					<xs:annotation>
						<xs:documentation>复审请求</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="opposition">
					<xs:annotation>
						<xs:documentation>撤销复审请求</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="invalidation">
					<xs:annotation>
						<xs:documentation>无效宣告请求</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="decisionType">
		<xs:simpleType>
			<xs:restriction base="xs:token">
				<xs:enumeration value="overrule">
					<xs:annotation>
						<xs:documentation>撤销原决定</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="affirmation">
					<xs:annotation>
						<xs:documentation>维持原决定</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="part-invalidation">
					<xs:annotation>
						<xs:documentation>无效宣告的部分无效</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="invalidation">
					<xs:annotation>
						<xs:documentation>全部无效</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="maintain">
					<xs:annotation>
						<xs:documentation>全部有效</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="verdictType">
		<xs:simpleType>
			<xs:restriction base="xs:token">
				<xs:enumeration value="first-instance"/>
				<xs:enumeration value="second-instance"/>
				<xs:enumeration value="review-instance"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="holdingType">
		<xs:simpleType>
			<xs:restriction base="xs:token">
				<xs:enumeration value="overrule"/>
				<xs:enumeration value="reaffirm"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attributeGroup name="language">
		<xs:annotation>
			<xs:documentation>语言和翻译方式</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="lang"/>
		<xs:attribute ref="format"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="source">
		<xs:attribute name="dataFormat">
			<xs:simpleType>
				<xs:restriction base="string">
					<xs:enumeration value="original"/>
					<xs:enumeration value="ipph"/>
					<xs:enumeration value="docdb"/>
					<xs:enumeration value="docdba"/>
					<xs:enumeration value="epodoc"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:simpleType name="Country">
		<xs:annotation>
			<xs:documentation>国家</xs:documentation>
		</xs:annotation>
		<xs:restriction base="string">
			<xs:pattern value="[A-Z]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Number">
		<xs:annotation>
			<xs:documentation>号码</xs:documentation>
		</xs:annotation>
		<xs:restriction base="string"/>
	</xs:simpleType>
	<xs:simpleType name="Kind">
		<xs:annotation>
			<xs:documentation>分类</xs:documentation>
		</xs:annotation>
		<xs:restriction base="string"/>
	</xs:simpleType>
	<xs:simpleType name="Date">
		<xs:annotation>
			<xs:documentation>日期</xs:documentation>
		</xs:annotation>
		<xs:restriction base="string"/>
	</xs:simpleType>
	<xs:simpleType name="lang">
		<xs:annotation>
			<xs:documentation>语言种类</xs:documentation>
		</xs:annotation>
		<xs:restriction base="string">
			<xs:pattern value="[a-z]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Text">
		<xs:annotation>
			<xs:documentation>无结构文本</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="string">
				<xs:attribute name="id" type="string"/>
				<xs:attribute ref="sequence"/>
				<xs:attributeGroup ref="language"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="string">
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Name">
		<xs:annotation>
			<xs:documentation>名字</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="string">
				<xs:attribute name="id" type="string"/>
				<xs:attribute ref="lang"/>
				<xs:attribute name="nameType">
					<xs:annotation>
						<xs:documentation>natural-自然人
legal-法人
</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:token">
							<xs:enumeration value="natural"/>
							<xs:enumeration value="legal"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
</xs:schema>
