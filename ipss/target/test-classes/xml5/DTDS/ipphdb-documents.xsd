<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:M="http://www.w3.org/1998/Math/MathML">
	<xs:import namespace="http://www.w3.org/1998/Math/MathML" schemaLocation="mathml3/mathml3.xsd"/>
	<xs:element name="Documents">
		<xs:annotation>
			<xs:documentation>根节点/root element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="PatentDocument">
					<xs:annotation>
						<xs:documentation>专利相关信息</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="CitingAndCited"/>
				<xs:element ref="PatentFamily"/>
				<xs:element ref="LegalDocument">
					<xs:annotation>
						<xs:documentation>中国法律状态</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="OperationalDocuments"/>
				<xs:element ref="FullDocImage"/>
				<xs:element ref="AppealDecisionDocument">
					<xs:annotation>
						<xs:documentation>中国复审决定信息</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="VerdictsDocument">
					<xs:annotation>
						<xs:documentation>中国司法判决信息</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:attribute name="id" type="string"/>
			<xs:attribute name="fileName" use="required">
				<xs:annotation>
					<xs:documentation>文件名</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute ref="quantity" use="required">
				<xs:annotation>
					<xs:documentation>子标签数量</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="schemaVersion" use="required"/>
			<xs:attribute name="producer" use="required" fixed="ipph"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="PatentDocument">
		<xs:annotation>
			<xs:documentation>主专利</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BibliographicData"/>
				<xs:element ref="Abstract" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="DesignBriefExplanation" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Claims" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Description" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Drawings" minOccurs="0"/>
				<xs:element ref="DesignPicture" minOccurs="0"/>
				<xs:element ref="DescriptionRelatedDocument" minOccurs="0"/>
				<xs:element ref="AmendedClaims" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AmendStatement" minOccurs="0">
					<xs:annotation>
						<xs:documentation>补正声明</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Keywords" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SearchReport" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SequenceLists" minOccurs="0"/>
				<xs:element ref="Chemistry" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Math" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Table" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="sequence" use="required"/>
			<xs:attributeGroup ref="source"/>
			<xs:attribute ref="status"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="BibliographicData">
		<xs:annotation>
			<xs:documentation>著录项目</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PublicationInfo" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="CaseNumber" type="string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>方案号</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CurrentLegalStatus" minOccurs="0">
					<xs:annotation>
						<xs:documentation>10：有权 
20：无权 
21：结合专利保护期限判断已经失效 
22：结合专利保护期限判断在审过期
30：在审</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="string">
							<xs:enumeration value="10"/>
							<xs:enumeration value="20"/>
							<xs:enumeration value="21"/>
							<xs:enumeration value="22"/>
							<xs:enumeration value="30"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="ApplicationInfo" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="RegistrationInfo" minOccurs="0"/>
				<xs:element name="DesignQuantity" type="xs:integer" minOccurs="0">
					<xs:annotation>
						<xs:documentation>申请中的外观设计数量</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DesignApplicationSource" type="string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>外观申请来源</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="RightsEffectiveDate" minOccurs="0"/>
				<xs:element name="OriginalApplicationLanguage" type="lang" minOccurs="0">
					<xs:annotation>
						<xs:documentation>原始申请语言，INID25</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ApplicationPublishingLanguage" type="lang" minOccurs="0">
					<xs:annotation>
						<xs:documentation>申请公布语言，INID26</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="PriorityClaimDetails" minOccurs="0"/>
				<xs:element ref="ExhibitionPriorityDetails" minOccurs="0"/>
				<xs:element ref="PublicDateAvailability" minOccurs="0">
					<xs:annotation>
						<xs:documentation>对公众有效期，INID40</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="ClassificationIPC" minOccurs="0">
					<xs:annotation>
						<xs:documentation>IPC分类，INID51</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="ClassificationsIPCR" minOccurs="0">
					<xs:annotation>
						<xs:documentation>IPCR分类</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="ClassificationsECLA" minOccurs="0">
					<xs:annotation>
						<xs:documentation>ELCA分类</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="ClassificationFIFT" minOccurs="0">
					<xs:annotation>
						<xs:documentation>FI-FT分类</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="ClassificationNational" minOccurs="0">
					<xs:annotation>
						<xs:documentation>国内/国家分类 INID52</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="ClassificationDomestic" minOccurs="0">
					<xs:annotation>
						<xs:documentation>内部分类号</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="PatentClassifications" minOccurs="0">
					<xs:annotation>
						<xs:documentation>CPC分类</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="USClassification" minOccurs="0"/>
				<xs:element ref="ClassificationsLocarno" minOccurs="0">
					<xs:annotation>
						<xs:documentation>洛迦诺分类</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="NationalEcoClassifications" minOccurs="0"/>
				<xs:element ref="Title" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>标题，INID54</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DesignDescription" type="Text" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>外观描述</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="SearchField" minOccurs="0">
					<xs:annotation>
						<xs:documentation>检索领域，INID58</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="RelatedDocuments" minOccurs="0">
					<xs:annotation>
						<xs:documentation>相关文档，INID60</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Parties" minOccurs="0">
					<xs:annotation>
						<xs:documentation>专利有关当事人，INID70</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Owners" minOccurs="0"/>
				<xs:element ref="CurrentOwners" minOccurs="0"/>
				<xs:element ref="HistoryOwners" minOccurs="0"/>
				<xs:element ref="Examiners" minOccurs="0">
					<xs:annotation>
						<xs:documentation>审查人 INID74</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Licensees" minOccurs="0"/>
				<xs:element ref="Designers" minOccurs="0"/>
				<xs:element ref="DesignationOfStates" minOccurs="0">
					<xs:annotation>
						<xs:documentation>协约指定的国家，INID81</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="BiologicalDeposit" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>微生物保藏信息，INID83</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="EntryNationalStageDate" type="Date" minOccurs="0">
					<xs:annotation>
						<xs:documentation>PCT22/39条款,完成日(进入国家阶段日期)，INID85</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="PCTorRegionalApplication" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>PCT或者地区申请信息，INID86</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="PCTorRegionalPublishing" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>PCT或者地区公开信息，INID87</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PCTPublicationLanguage" type="lang" minOccurs="0">
					<xs:annotation>
						<xs:documentation>国际公布语言</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="ST50Publication" minOccurs="0"/>
				<xs:element ref="DesignModifications" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ReferencesCited" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ForwardCit" minOccurs="0"/>
				<xs:element name="SearchReportDelayPub" minOccurs="0">
					<xs:annotation>
						<xs:documentation>检索报告的延迟公布</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Date" type="Date"/>
							<xs:element ref="GazetteReference" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DesignProductDescription" type="Text" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>外观设计产品描述</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="NoveltyLack" minOccurs="0">
					<xs:annotation>
						<xs:documentation>外观设计缺乏新颖性声明</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Text" type="Text" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="AdvancedPublishedDocument" minOccurs="0">
					<xs:annotation>
						<xs:documentation>是否提前公开</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="string">
							<xs:enumeration value="Y"/>
							<xs:enumeration value="N"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="CurrentLegal" minOccurs="0"/>
				<xs:element ref="SpecificData" minOccurs="0">
					<xs:annotation>
						<xs:documentation>各国和组织的特有著录项目</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="StatisticalInfo" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute ref="lang"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="PublicationInfo">
		<xs:annotation>
			<xs:documentation>公开信息，INID10</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DocumentID"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute name="status"/>
			<xs:attribute ref="sequence"/>
			<xs:attribute ref="lang"/>
			<xs:attributeGroup ref="source"/>
			<xs:attribute name="isRepresentative"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ApplicationInfo">
		<xs:annotation>
			<xs:documentation>申请信息，INID20</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DocumentID"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="sequence"/>
			<xs:attribute name="applType"/>
			<xs:attribute name="status"/>
			<xs:attributeGroup ref="source"/>
			<xs:attribute name="isRepresentative"/>
			<xs:attribute name="docID"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="RegistrationInfo">
		<xs:annotation>
			<xs:documentation>外观注册信息</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="RegistrationNumber" type="Number" minOccurs="0">
					<xs:annotation>
						<xs:documentation>外观注册号</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RegistrationDate" type="Date" minOccurs="0">
					<xs:annotation>
						<xs:documentation>外观注册日</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="sequence"/>
			<xs:attributeGroup ref="source"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="RightsEffectiveDate">
		<xs:annotation>
			<xs:documentation>产权生效日，INID24</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CommingIntoForce" minOccurs="0">
					<xs:annotation>
						<xs:documentation>生效日期</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="Date">
								<xs:attribute name="status" type="string"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="RequestForExamination" type="Date" minOccurs="0">
					<xs:annotation>
						<xs:documentation>实审请求日,ST32-B241</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DespatchOfFirstExaminationReport" type="Date" minOccurs="0">
					<xs:annotation>
						<xs:documentation>第一次审查通知书发出日,ST32-B242</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PatentMaintainedAsAmended" type="Date" minOccurs="0">
					<xs:annotation>
						<xs:documentation>按照更正维持专利日,ST32-B243</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RequestForConversionToNationalApplication" minOccurs="0">
					<xs:annotation>
						<xs:documentation>转换为国家申请的请求 ,ST32-B244</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Date" type="Date" minOccurs="0"/>
