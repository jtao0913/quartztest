								</xs:sequence>
								<xs:attribute ref="organizationType"/>
								<xs:attribute ref="designation"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="DeceasedInventor" minOccurs="0" maxOccurs="unbounded">
