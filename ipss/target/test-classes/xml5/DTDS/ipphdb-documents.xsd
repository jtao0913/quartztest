									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="RefFileArticlePages">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="PageRange"/>
									</xs:sequence>
								</xs:complexType>
