					</xs:complexType>
				</xs:element>
				<xs:element name="LicensingInfo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>专利许可</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="LicensingRecord">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Licensing" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="LicensingDetail">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="LicensingKind" minOccurs="0">
