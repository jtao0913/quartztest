							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="Person">
										<xs:sequence>
											<xs:element name="Text" type="Text" minOccurs="0"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="LegalAttorneyDetails">
		<xs:annotation>
			<xs:documentation>法律代理人信息</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="LegalAttorney" maxOccurs="unbounded">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="Person">
								<xs:sequence>
									<xs:element name="Text" type="Text" minOccurs="0"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GazetteReference">
		<xs:annotation>
			<xs:documentation>专利公告模块,ST.32-B405</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="GazetteNumber" type="Number" minOccurs="0"/>
				<xs:element name="Date" type="Date" minOccurs="0"/>
				<xs:element name="Text" type="Text" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="lang"/>
			<xs:attributeGroup ref="source"/>
			<xs:attribute name="country" type="Country"/>
