						<xs:documentation>获取途径</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Class" minOccurs="0">
					<xs:annotation>
						<xs:documentation>分类信息</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="string">
								<xs:attribute name="type"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="Keyword" type="Text" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>关键词</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Note" minOccurs="0">
					<xs:annotation>
						<xs:documentation>注释</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="string">
								<xs:attribute ref="lang"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="Avail" type="string" minOccurs="0"/>
				<xs:element name="History" minOccurs="0">
					<xs:complexType>
						<xs:choice>
							<xs:element name="Text" type="Text"/>
							<xs:choice>
								<xs:element name="Received">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Date" type="Date"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="Accepted">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Date" type="Date"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="Revised">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Date" type="Date"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="MISC">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Date" type="Date"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:choice>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element name="CopyRight" type="Text" minOccurs="0">
					<xs:annotation>
						<xs:documentation>版权</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DOI" type="string" minOccurs="0"/>
				<xs:element name="ISSN" type="string" minOccurs="0"/>
				<xs:element name="ISBN" type="string" minOccurs="0"/>
				<xs:element name="CitDate" type="Date" minOccurs="0">
					<xs:annotation>
						<xs:documentation>引用日期</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SearchTerm" type="string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>检索项</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DescriptiveText" type="Text" minOccurs="0"/>
				<xs:element name="ReferenceNumber" type="string" minOccurs="0"/>
				<xs:sequence minOccurs="0">
					<xs:element ref="Article" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="Serial" minOccurs="0"/>
					<xs:element ref="Book" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="Conference" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="TechnicalStandard" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="OtherCitation" minOccurs="0"/>
				</xs:sequence>
				<xs:element ref="RichContent" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="Text" type="Text" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="OtherCitation">
		<xs:annotation>
			<xs:documentation>其他引证</xs:documentation>
