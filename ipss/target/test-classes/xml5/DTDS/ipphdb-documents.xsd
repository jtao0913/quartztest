						<xs:attribute name="id" type="string"/>
					</xs:complexType>
				</xs:element>
				<xs:element ref="Keywords" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="id" type="string"/>
			<xs:attribute ref="sequence"/>
			<xs:attributeGroup ref="language"/>
			<xs:attribute name="correction" use="optional">
				<xs:simpleType>
					<xs:restriction base="string">
						<xs:enumeration value="Y">
