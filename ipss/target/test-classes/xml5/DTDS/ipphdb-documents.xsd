						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="DomesticPriorityClaim" maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Substituted" maxOccurs="unbounded">
