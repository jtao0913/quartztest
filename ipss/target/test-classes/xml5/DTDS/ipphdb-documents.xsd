					</xs:annotation>
				</xs:element>
				<xs:element name="VolumeNumber" type="string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>卷号</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="IssueNumber" type="string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>发行号</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Series" minOccurs="0"/>
				<xs:element name="AvailableSource" type="string" minOccurs="0">
