																		<xs:documentation>许可种类</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ContractRecodationStage">
																	<xs:annotation>
																		<xs:documentation>“生效” or “变更” or “注销”</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:choice>
																	<xs:element name="RecordationDate">
																		<xs:annotation>
																			<xs:documentation>生效日</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="ChangeDate">
																		<xs:annotation>
																			<xs:documentation>变更日</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="ExemptionDate">
																		<xs:annotation>
																			<xs:documentation>解除日</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:choice>
																<xs:element name="ContractRecordationNumber">
																	<xs:annotation>
																		<xs:documentation>合同备案号</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ContractFulfillmentPeriod" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>合同履行期限</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="Licensors" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>让与人</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="Licensor" maxOccurs="unbounded">
																				<xs:complexType>
																					<xs:complexContent>
																						<xs:extension base="Person">
																							<xs:attribute ref="organizationType"/>
																							<xs:attribute ref="active"/>
																						</xs:extension>
																					</xs:complexContent>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="Licensees" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>受让人</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="Licensee" maxOccurs="unbounded">
																				<xs:complexType>
																					<xs:complexContent>
																						<xs:extension base="Person">
																							<xs:attribute ref="organizationType"/>
																							<xs:attribute ref="active"/>
																						</xs:extension>
																					</xs:complexContent>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element ref="Legal"/>
												</xs:sequence>
												<xs:attribute ref="sequence"/>
												<xs:attribute name="eventDate">
													<xs:annotation>
														<xs:documentation>时间公开日</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute ref="sequence"/>
			<xs:attribute name="patentType"/>
			<xs:attribute ref="status"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FullDocImage">
